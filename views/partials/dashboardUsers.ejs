<div class="dash-item">
  <table class="table">
    <h4 style="padding:.5em;">Assigned Tests</h4>
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <th scope="col">Test</th>
          <th scope="col">Result</th>
          <th scope="col">Completed</th>
        </tr>
      </thead>
      <tbody>
      <% for(let i = 0; i < result.length; i++){%>
        <tr>
          <th scope="row"><%=result[i].user_Name%></th>
          <td><%=result[i].email%></td>
          <td><%=result[i].name%></td>
          <td><%=result[i].correct%>/<%=result[i].total%></td>
          <% if (result[i].completed) { %>
            <td><div class="status-complete">Completed</div></td>
          <% } else { %>
            <td><div class="status-notstarted">Not Started</div></td>
          <% } %>
        </tr>
        <% } %>
      </tbody>
    </table>
             <%if (result.length == 0) { %>
       You don't have any Users!
        <% } %>
</div>
